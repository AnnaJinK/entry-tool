{
    "plugins": [
        "@babel/plugin-transform-object-assign",
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "@actions": "./src/actions",
                    "@assets": "./src/assets",
                    "@components": "./src/components",
                    "@utils": "./src/utils"
                }
            }
        ]
    ],
    "presets": ["@babel/preset-react"]
}
